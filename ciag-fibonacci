#include <stdio.h>

int fib(int x);
void drukuj(int x);

void main()
{
  int a=40;
  
  drukuj(a);

  return;
}
int fib(int x)
{
  int i, f;

    if (x==0)
    f=0;
    else
    {
      if (x==1)
      f=1;
        else
        {
        f=fib(x-1)+fib(x-2);
        }
    }

  return f;
}
void drukuj(int x)
{
  int i;
  for (i=0; i<=x; i++)
    printf("%d\n", fib(i));

  return 0;
}
