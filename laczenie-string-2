#include <stdio.h>
#include <string.h>

int main(void) {


  char napis1[20];
  char napis2[20];
  
  scanf("%s", napis1);
  scanf("%s", napis2);
  
  printf("napis 1: %s\n", napis1);
  printf("napis 2: %s\n", napis2);

  strcat(napis1, napis2);

  printf("połączone: %s\n", napis1);
  
  return 0;
}
